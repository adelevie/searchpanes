<html>

<head>
<link rel="stylesheet" type="text/css" href="style.css" />
<title>Test Input</title>


<script type="text/javaScript">

       function processFormData() {
	
               var left_url;
               var right_url;
               left_url = "/fcc/" + document.getElementById("term").value;
               right_url = "/searchusa/" + document.getElementById("term").value;

               var left_frame;
               var right_frame;
               left_frame = document.getElementById("left_frame");
               right_frame = document.getElementById("right_frame");



               right_frame.src = right_url;
			   left_frame.src = left_url;

       }
</script>
</head>

<body>

<form id="form" action="#">

Term: <input type="text" name="term" id="term" />

<input type="button" name="submit" value="Submit"
onclick="processFormData();" />
</form>

<table width="100%" height="100%" border="1">
 <tr>
   <td><iframe class="frame" id="left_frame" name="left_frame"> </iframe></td>
   <td><iframe class="frame" id="right_frame" name="right_frame"> </iframe></td>
 </tr>
</table>

</body>
</html>